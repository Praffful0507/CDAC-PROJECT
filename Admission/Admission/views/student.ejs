  <body class="text-center">
    <form class="form-signin" action="/" method="post">
      <img class="mb-4" src="images/coffe.png" alt="" width="72" height="72">
      <h1 class="h3 mb-3 font-weight-normal">sign-UP the page</h1>
      
      <label for="inputFirstName" class="sr-only">First Name</label>
      <input type="text" id="inputFirstName" class="form-control" placeholder="First Name" required autofocus name="FirstName">
      <label for="inputLastName" class="sr-only">lastName</label>
      <input type="text" id="inputLastName" class="form-control" placeholder="lastName" required name="LastName">
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="text" id="inputEmail" class="form-control" placeholder="Email address" required name="Email">
      <label for="inputEmail" class="sr-only">User Name</label>
      <input type="text" id="inputEmail" class="form-control" placeholder="Email address" required name="Email">
      <label for="inputEmail" class="sr-only">Password</label>
      <input type="text" id="inputEmail" class="form-control" placeholder="Email address" required name="Email">


        <div class=".checkbox"></div>
      <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
      <p class="mt-5 mb-3 text-muted">&copy; 2021-2022</p>
    </form>
  </body>
